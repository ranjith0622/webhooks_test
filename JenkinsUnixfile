pipeline {
    agent any

    stages {
        stage('Test') {
            steps {
                 echo "Testing"
                 sh 'call ant test -buildfile buildut.xml'
                        
            }
        }
        stage('Build') {
            steps {
                 echo "Build"
                 sh 'call ant' 
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
